/***************************************************************************/
/* Progress Bar                                                            */
/***************************************************************************/

.assessment-progress-status-main-container{
    position:fixed;
    top:90vh;
    left:10vw;
    opacity:.6;
    width:80vw;
    transition:all 500ms;
}
.assessment-progress-status-main-container[new],
.assessment-progress-status-main-container:hover{
    opacity:1;
}

.progress{
    transition:all 500ms;
    white-space:nowrap;
    height:40px;
    box-shadow:10px 20px 5px rgba(0,0,0,.5),inset 0px 1px rgba(255,255,255,.6);
}
.progress-bar{
    transition:all 500ms;
    line-height:40px;
    font-size:20px;
}
.progress-bar-finished,
.progress-bar-locked{
    background: #57a900;
}
.progress-bar-unseen{
    background: #a94442;
}
.progress-bar-seen{
    background: #3498db;
}



.main-status-and-submition-container .submit-assessment-container{
    position:fixed;
    text-align:center;
    top:150vh;
    left:25vw;
    opacity:.1;
    width:50vw;
    transition:all 500ms;
}
.main-status-and-submition-container[ready-to-submit] .submit-assessment-container{
    top:80vh;
    opacity:1;
}
.main-status-and-submition-container[ready-to-submit] .assessment-progress-status-main-container{
    top:150vh;
    opacity:.1;
}
.main-status-and-submition-container .submit-assessment-container{
    cursor:pointer;
    padding:20px;
    font-size:50px;
    font-family:"Trajan Pro";
    border-radius:10px;
    box-shadow:4px 30px 10px rgba(0,0,0,.1),inset 0px 1px 0px rgba(255,255,255,.7);
    background:#5aaf00;
    color:white;
    font-weight:bold;
    border:1px solid rgba(0,0,0,.1);
}
.main-status-and-submition-container .submit-assessment-container:hover{
    box-shadow:4px 10px 10px rgba(0,0,0,.1),inset 0px 1px 40px rgba(255,255,255,1);
}


.assessment-report-info{
