[api-component="QuestionTypes"] [api-attr-array-index]{
    padding:10px 10px;
    max-height:70vh;
    overflow:auto;
    border-radius:10px;
    box-shadow:0px 1px 1px rgba(0,0,0,.6),inset 0px 1px 0px rgba(255,255,255,.8);
    margin:10px 1px;
    cursor:pointer;
    font-family:"Adobe Caslon Pro";

    transition:all 500ms;
}
[api-component="QuestionTypes"]  [api-attribute="Name"]{
    font-size:20px;
    text-decoration:underline;
}
[api-component="QuestionTypes"]  .contactInfo{
    font-family:"Adobe Caslon Pro";
}
[api-component="QuestionTypes"]  .contactInfo [api-attribute]
{
    display:inline-block;
}

[api-component="QuestionTypes"] [api-attr-array-index]:hover{
    box-shadow:0px 1px 1px rgba(0,0,0,.6),inset 0px 1px 30px rgba(218, 218, 218, 0.97);
}
[api-component="QuestionTypes"] [api-attr-array-index]:focus{
    box-shadow:0px 0px 10px rgba(0,0,0,.6),inset 0px 1px 30px #337ab7;
    outline:none;
}
[api-component="QuestionTypes"] [api-attr-array-index][is-selected]{
    box-shadow:0px 0px 10px rgba(0,0,0,.6),inset 0px 1px 30px #9CCC65;
}

.question-and-types-container{
    position:relative;
}
.question-and-types-container .questions-container{
    display:inline-block;
    width:100%;

    transition:all 500ms;
}
.question-and-types-container[is-selected] .questions-container{
    display:inline-block;
    width:70%;
    transition:all 500ms;
}
.question-and-types-container .question-type-container{
    display:inline-block;
    width:20vw;

    float:right;
    transition:all 500ms;
    position:absolute;
    transform:translateX(0%);
    left:100%;
    top:-5vh;
    opacity:0;
}
.question-and-types-container[is-selected] .question-type-container{
    display:inline-block;
    float:right;
    transition:all 500ms;
    position:absolute;
    left:100%;
    transform:translateX(-100%);
    top:-5vh;
    opacity:1;

}
.question-and-types-container[is-selected] .question-type-container .options {

    border-top:1px solid rgba(0,0,0,.1);
    padding:10px;
    text-align:center;
    background:transparent;

}

.question-and-types-container[is-selected] .question-type-container .options .make-a-question-btn:before{
    content:"Create a Question";

    /* line-height:20px; */
}
.question-and-types-container[is-selected] .question-type-container .options .make-a-question-btn{
    font-size:20px;
}
.question-and-types-container[is-selected] .question-type-container .options:hover{
    font-weight:bold;
    text-decoration:underline;
}