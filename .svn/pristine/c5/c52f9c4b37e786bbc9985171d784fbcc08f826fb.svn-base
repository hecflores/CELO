}


.assessment-report-questions-main-container[filter='wrong-only'] .show-correct-only-btn,
.assessment-report-questions-main-container[filter='wrong-only'] .show-all-btn{
    display:inline-block;
}
.assessment-report-questions-main-container[filter='wrong-only'] .show-incorrect-only-btn{
