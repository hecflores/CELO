/***************************************************************************/
/*                          L o g i n   P a g e                            */
/***************************************************************************/
.container .login-form-main
{
    max-width: 25vw;
    color:#333;
    -webkit-border-radius:10px;
    -moz-border-radius:10px;
    border-radius:10px;
}
.container .login-form-main .login-form-title-container
{
    color:#FFF9D9;
    background: #c8102e;
    border-radius: 10px;
    box-shadow: 0px 3px 5px rgba(0,0,0,.5), inset 0px 3px rgba(255,255,255,.5);
    margin-bottom: 15px;
}
.container .login-form-main .login-form-title-container .login-form-title
{
    font-weight: bold;
    text-align: center;
    margin: 0px;
    padding: 20px;
}

.container .login-form-main .login-form-container
{
    color: #333;
    background: #e3e3e3;
    border-radius: 10px;
    box-shadow: 0px 3px 5px rgba(0,0,0,.5), inset 0px 3px rgba(255,255,255,.5);
    padding: 40px;
}

.container .login-form-main .login-form-container .login-form-submit-container
{
    margin-bottom: 0px;
    text-align: center;
}
.container .login-form-main .login-form-container input[type='submit']
{
    height: 34px;
    width: 190px;
    border: medium none;
    color: #FFFFFF;
    font-weight: bold;
    text-align: center;
    max-width: 190px !important;
    background: url(https://accessuh.uh.edu/img/bg-button-ps.png) no-repeat scroll 0 0 transparent !important;
}
.user_searchItem{
    cursor:pointer;
    background:rgba(255,255,255,1);
    -webkit-transition: all 200ms;
    -moz-transition: all 200ms;
    -ms-transition: all 200ms;
    -o-transition: all 200ms;
    transition: all 200ms;
}
.user_searchItem:hover{
    background:rgba(240,240,240,1);
}

.content-item:active,
.content-item:visited,
.content-item{
    padding: 10px 15px;
    border: 1px solid #ddd;
    margin-bottom: -1px;
    background:#f5f5f5;
    cursor:pointer;
    color: #555;
}
.content-item:hover{
    background:rgba(240,240,240,.9);
    color: #555;
}
.no-hover-background,
.no-hover-background:hover{
    background:white !important;
    border:2px solid #337ab7;
    padding: 4px 10px;
    font-weight: bold;
}
.tag-action{
    display:inline-block;
    min-width:10px;
    min-height:10px;
}
.tag-action-item-new,
.tag-action-item {
    cursor:pointer;
    padding:5px 10px;
    margin:2px;
    border-radius:10px;
    background: rgb(51, 122, 183);
    color:white;
}
.tag-action-item-new {
    background: rgb(87, 169, 0);
}
.tag-action-item-new:hover{
    background: rgb(67, 149, 0);
}
.tag-action-item:hover{
    background: rgb(12, 102, 153);
}
.tag-action-item .deleteTag{
    margin-left:10px;
    vertical-align: text-top;
}
.tag-action-item-create {
    cursor:pointer;
    padding:5px 10px;
    margin:2px;
    border-radius:10px;
    background: rgb(51, 122, 183);
    color:white;
}
.tag-action[tag-nonew]  .tag-action-item-new{
    display:none;
}
.tag-action[no-delete] .deleteTag{
    display:none;
}
.questions-table tr{
    cursor:pointer;
}
.questions-table tr:hover{
    background:rgb(240,240,240);
    color:rgb(10,10,10);
}
.magic-points-board{
    position:relative;
    left:50%;
    transform:translateX(-50%);
    margin-top:20px;
}
.magic-points-grid-cell{
    display:inline-block;
    position:absolute;
    box-shadow:0px 1px 2px rgba(0,0,0,.6),inset 0px 2px rgba(255,255,255,.7);
    border-radius: 10px;
    transition:all 500ms;
    cursor:pointer;
    border:2px solid transparent;
}
.magic-points-grid-cell:hover{
    border-color:rgb(130,255,130);
}
.magic-points-grid-cell[answered='true']{
    border-color:rgb(130,130,255);
}
.magic-points-grid-cell[correct='true']{
    background:rgb(130,255,130);
}
.magic-points-grid-cell[answered='true'][correct='false']{
    background:rgb(255,130,130);
}

.graded-question{
    padding:20px;
    margin:10px;
    border-radius: 10px;
    -webkit-box-shadow: 0px 1px 5px rgba(0,0,0,.5),inset 0px 2px rgba(255,255,255,.8);
    -moz-box-shadow: 0px 1px 5px rgba(0,0,0,.5),inset 0px 2px rgba(255,255,255,.8);
    box-shadow: 0px 1px 5px rgba(0,0,0,.5),inset 0px 2px rgba(255,255,255,.8);
}


/***************************************************************************/
/* JSON Formatted Text                                                     */
/***************************************************************************/
pre {outline: 1px solid #ccc; padding: 5px; margin: 5px; }
.string { color: green; }
.number { color: darkorange; }
.boolean { color: blue; }
.null { color: magenta; }
.key { color: red; }


@keyframes spin {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
}
.loading-helper{
    text-align:center;
}
.loading-spinner {
    display:inline-block;
    content: '';
    box-sizing: border-box;
    position: absolute;
    width: 20px;
    height: 20px;
    margin-top: 0px;
    margin-left: 0px;
    border-radius: 50%;
    border-top: 5px solid #07d;
    border-right: 5px solid transparent;
    animation: spin 1s linear infinite;
}
.loading-splash-screen{
    background:rgba(0,0,0,.6);
    position:fixed;
    display:block;
    width:100%;
    height:100%;
    top:0;
    left:0;
    z-index:1000;
    opacity:1;
    transition-property: opacity, z-index;
    transition-duration: 500ms  , 1ms;
    transition-delay:    1ms    , 0ms;

}
.loading-spinner-container{
    text-align:center;
    display:inline-block;
    position:relative;
    left:50%;
    top:50%;
    transform:translate(-50%,-50%);
}
.loading-splash-screen .loading-spinner{
    width: 40vw;
    height: 40vw;
    left:0;
    top:0;
    border-width: 15px;


    position:relative;
}

.loading-splash-screen.disabled{
    opacity:0;
    z-index:-1;
    transition-property: opacity, z-index;
    transition-duration: 500ms  , 1ms;
    transition-delay:    1ms    , 501ms;

}
.loading-spinner-container  .loading-message {

    font-size:45px;
    font-family:"Adobe Caslon Pro";
    /*position:absolute;*/
    /*left:50%;*/
    /*top:50%;*/
    color:white;
    padding:10px;
    border-radius:10px;
    /*box-shadow:0px 1px 20px rgba(0,0,0,.9),inset 0px 1px rgba(255,255,255,.8);*.
    /*transform:translate(-50%,-50%);*/

}



