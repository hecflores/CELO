/***************************************************************************/
/* Course Content Breadcrumb Improving. We will have a trigger to keep     */
/* of children                                                             */
/***************************************************************************/
DROP TABLE CourseContentChildren;
CREATE TABLE CourseContentChildren(
  ContentID BIGINT,
  ChildContentID BIGINT,
  Depth INTEGER
);

DELIMITER //
DROP TRIGGER CourseContentChildren;
CREATE TRIGGER CourseContentChildren AFTER INSERT ON CourseContent
  FOR EACH ROW
  BEGIN
    DECLARE ParentID BIGINT;
    DECLARE Depth INTEGER;

    /***********************************************************************/
    /* Get the Parent Id of this course                                    */
    /***********************************************************************/
    SET ParentID=new.ParentFolderID;
    SET Depth=new.Depth;

    /***********************************************************************/
    /* Found it so now we will add it to all parents                       */
    /***********************************************************************/
    IF ParentID IS NOT NULL THEN

      INSERT INTO CourseContentChildren (ContentID, ChildContentID, Depth)
        SELECT S1.ContentID,(new.ContentID) AS ChildContentID,(new.Depth-S1.Depth) AS Depth FROM CourseContentChildren AS S1
          WHERE (S1.ChildContentID = new.ParentFolderID );
      INSERT INTO CourseContentChildren (ContentID, ChildContentID, Depth) VALUES
        (new.ParentFolderID,new.ContentID,new.Depth);



    END IF;
  END//
DELIMITER ;

DROP VIEW CourseContentBreadcrumbs;
CREATE VIEW CourseContentBreadcrumbs AS
  SELECT
    S2.ContentID,                             /* From Child Table */
    S1.ParentFolderID AS PreviousContentID,
    S1.ContentID AS CurrentContentID,
    S2.ChildContentID,
    S1.SectionID,
    S1.Name,
    S1.URL,
    S1.Description,
    S1.ParentFolderID,
    S1.RootContentID,
    S1.Depth,
    S1.Type,
    S1.Properties,
    S1.Gradable,
    S1.CreatedBy,
    S1.RolesVisible

  FROM CourseContentChildren AS S2
    JOIN CourseContent AS S1 ON S1.ContentID=S2.ContentID;

/***************************************************************************/
/* Amazing View!. Will Give you the Grade of any section of Course Content */
/* Which includes the trees underneath...Per User! This is amazing!        */
/***************************************************************************/
DROP VIEW GradeProgress_CourseContentTree;
CREATE VIEW GradeProgress_CourseContentTree AS
  SELECT S1.ContentID,S1.ChildContentID, S4.Name,S4.Type,S4.Depth, S7.UserID

    ,(CASE WHEN S3.PointsEarned IS NULL THEN 0 ELSE ((SUM(

                                                          S3.PointsEarned
                                                      )/SUM(S3.PointsWorth
                                                      ))*100.0) END) AS Grade
  FROM CourseContentBreadcrumbs AS S1
    LEFT JOIN CourseContent AS S4 ON S4.ContentID=S1.ChildContentID
    LEFT JOIN  AnswerableQuestionsGroups AS S2 ON S2.CourseContentID=S1.ChildContentID
    LEFT JOIN AnsweredQuestionsReport  AS S3 ON S3.AnsweredGroupID=S2.AnsweredGroupID,
    Users AS S7
  WHERE S7.UserID=S3.UserID
  GROUP BY S1.ContentID,S1.ChildContentID,S1.Name,S1.Type,S1.Depth,S7.UserID;

/***************************************************************************/
  SELECT S1.ContentID,S1.ChildContentID, S4.Name,S4.Type,S4.Depth, S3.UserID

    ,((SUM(
           CASE WHEN S3.PointsEarned IS NOT NULL THEN
             S3.PointsEarned
           ELSE
             0
           END
       )/SUM(
           CASE WHEN S3.PointsWorth IS NOT NULL THEN
             S3.PointsWorth
           ELSE
             0
           END
       ))*100.0) AS Grade
  FROM CourseContentBreadcrumbs AS S1
    LEFT JOIN CourseContent AS S4 ON S4.ContentID=S1.ChildContentID
    LEFT JOIN  AnswerableQuestionsGroups AS S2 ON S2.CourseContentID=S1.ChildContentID
    LEFT JOIN AnsweredQuestionsReport  AS S3 ON S3.AnsweredGroupID=S2.AnsweredGroupID AND S1.ContentID=10
  GROUP BY S1.ContentID,S1.ChildContentID,S1.Name,S1.Type,S1.Depth, S3.UserID;
/***************************************************************************/


/***************************************************************************/
/* Amazing View!. Will Give you the Grade of any section of Course Content */
/* Which includes the trees underneath...Per User! This is amazing!        */
/***************************************************************************/
DROP VIEW GradeProgress_CourseContentTree;
CREATE VIEW GradeProgress_CourseContentTree AS
  SELECT S1.ContentID,S1.Name,S1.Type,S1.Depth, S3.UserID

    ,((SUM(
           CASE WHEN S3.PointsEarned IS NOT NULL THEN
             S3.PointsEarned
