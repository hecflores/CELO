.spider_house{
    transition: all 500ms;
    /* animation-name: fadeInAnimation,RotateConstant,RotateConstant; */
    animation-duration: 3s,1s,40s;
    animation-iteration-count: 1,1,infinite;
    animation-timing-function: cubic-bezier(0, 0, 0, 1.2),cubic-bezier(0, 0, 0, 1.2),linear;
    animation-delay: 3s,3s,4s;
    /* width: 50vw; */
    /* height: 50vw; */
