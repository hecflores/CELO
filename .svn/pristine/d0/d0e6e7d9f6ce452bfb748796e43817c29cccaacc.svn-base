/***************************************************************************/
/* Progress Bar                                                            */
/***************************************************************************/

.assessment-progress-status-main-container{
    position:fixed;
    top:90vh;
    left:10vw;
    opacity:.6;
    width:80vw;
    transition:all 500ms;
}
.assessment-progress-status-main-container[new],
.assessment-progress-status-main-container:hover{
    opacity:1;
}

.assessment-progress-status-main-container  .progress{
    transition:all 500ms;
    white-space:nowrap;
    height:40px;
    box-shadow: 10px 20px 5px rgba(0,0,0,.5), inset 0px 1px rgba(255,255,255,.6);
    background: #FFFAFF;
    border: 1px solid rgba(0,0,0,.1);
}
.assessment-progress-status-main-container .progress-bar{
    transition:all 500ms;
    line-height:40px;
    font-size:20px;
    font-family:cursive;
}
.progress-bar.progress-bar-finished{
    position:relative;
    overflow:visible;
}
.progress-bar.progress-bar-finished:after{
    position:absolute;
    top:0px;
    display:inline-block;
    opacity:0;
    margin-left:3%;
    color:#0A2463;
    transition:all 500ms;
    font-size:20px;
    left:100%;
    content:'Finished';
    font-family:cursive;
}
.assessment-progress-status-main-container:hover .progress-bar.progress-bar-finished:after{
    opacity:1;
}
.progress-bar.progress-bar-finished,
.progress-bar.progress-bar-locked{
    background: #57a900;
}
.progress-bar.progress-bar-unseen{
    background: #a94442;
}
.progress-bar.progress-bar-seen{
    background: #3498db;
}



