}
.container .login-form-main .login-form-title-container
{
    color:#FFF9D9;
    background: #c8102e;
    border-radius: 10px;
    box-shadow: 0px 3px 5px rgba(0,0,0,.5), inset 0px 3px rgba(255,255,255,.5);
    margin-bottom: 15px;
}
.container .login-form-main .login-form-title-container .login-form-title
{
    font-weight: bold;
    text-align: center;
    margin: 0px;
    padding: 20px;
}

.container .login-form-main .login-form-container
{
    color: #333;
    background: #e3e3e3;
