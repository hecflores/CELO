.spider_house_progress:before{
    content: " ";
    outline: 5px solid red;
}
.spider_house_progress{
    border-top: 25vw solid red;
    border-left: 2vw solid rgba(217, 237, 247, 0);
    border-right: 2vw solid rgba(217, 237, 247, 0);
    height:0px;
    transform-origin:50% 100%;
    transform:rotateZ(0deg);
    transition:all 500ms;
    width:4vw;
    position:absolute;
    left:45%;
}
.spider_house:hover .spider_house_progress:nth-child(1){
    transform:rotateZ(10deg);
}
.spider_house:hover .spider_house_progress:nth-child(2){
    transform:rotateZ(30deg);
}
.spider_house:hover .spider_house_progress:nth-child(3){
    transform:rotateZ(50deg);
}
.spider_house:hover .spider_house_progress:nth-child(4){
    transform:rotateZ(70deg);
}
.spider_house:hover .spider_house_progress:nth-child(5){
    transform:rotateZ(90deg);
}
.spider_house:hover .spider_house_progress:nth-child(6){
    transform:rotateZ(110deg);
}

