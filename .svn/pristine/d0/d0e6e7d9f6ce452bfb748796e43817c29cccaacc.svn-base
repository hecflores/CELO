[api-component="CourseContent"]{
    padding:10px 0px;
}
[api-component="CourseContent"] [api-attribute="Name"]{
    font-size:30px;
    font-family:"Trajan Pro";
    font-weight:bold;
    text-align:center;
}

[api-component="CourseContent"] [api-attribute="Description"]{
    font-size:15px;
    font-family:"Trajan Pro";
    margin-left:3px;
    margin-bottom:15px;
    text-align:center;
}


[api-component="CourseContent"] [api-attribute='Children']{
    padding:10px 0px;
}

[api-component="CourseContent"] [api-attribute='Children'] [api-attr-array-index]{

    box-shadow:0px 1px 4px rgba(0,0,0,.6),inset 0px 1px 0px rgba(255,255,255,.7);
    margin:10px 0px;
    border-radius:5px;
    padding:10px;
    cursor:pointer;
    transition:all 500ms;
}
[api-component="CourseContent"] [api-attribute='Children'] [api-attr-array-index]:focus{
    box-shadow:1px 2px 6px rgba(0,0,0,.6),inset 0px 1px 60px #3498db !important;
}
[api-component="CourseContent"] [api-attribute='Children'] [api-attr-array-index]:hover{
    box-shadow:1px 2px 6px rgba(0,0,0,.6),inset 0px 1px 60px rgba(87, 169, 0, .5);
}

[api-component="CourseContent"] [api-attribute='Children'] [api-attribute='Children->Name']{
    font-size:25px;
    font-family:"Trajan Pro";

}
[api-component="CourseContent"] [api-attribute='Children'] [api-attribute='Children->Name']{
    margin-bottom:15px;
    border-bottom:1px solid #ddd;
}
[api-component="CourseContent"] [api-attribute='Children'] [api-attribute='Children->Description']{
    font-family:"Adobe Caslon Pro";
}



.delete-btn{
    display:inline-block;
    width:20px;
    height:20px;
    background:red;
    border-radius:20px;
    line-height:20px;
    font-size:15px;
    text-align:center;
    color:white;
    font-weight:bold;
    box-shadow:0px 1px 1px rgba(0,0,0,.6),inset 0px 1px 0px rgba(255,255,255,0);
    transform:translateX(40px);
    display:none;
    transition:all 500ms;
}
.delete-btn:hover{
    box-shadow:0px 1px 1px rgba(0,0,0,.6),inset 0px 1px 20px #ff5956;
}
.delete-btn:before{
    content:"X";
}
.top-right-info-container{
    float:right;
}


[api-component="CourseContent"] [api-attribute="Breadcrums"]{
    padding:0px 0px;
    display:inline-block;
    box-shadow:0px 1px 1px  rgba(0,0,0,.7),inset 0px 1px rgba(255,255,255,.7);
    border-radius:5px;
    background:#337ab7;
    color:white;
}
[api-component="CourseContent"] [api-attribute="Breadcrums"] [api-attr-array-index]:hover{
    box-shadow:inset 0px 0px 30px #333;
}

[api-component="CourseContent"] [api-attribute="Breadcrums"] [api-attr-array-index]:first-child{
    border-top-left-radius:5px;
    border-bottom-left-radius:5px;
}
[api-component="CourseContent"] [api-attribute="Breadcrums"] [api-attr-array-index]:last-child{
    border-top-right-radius:5px;
    border-bottom-right-radius:5px;
    border:none;
    box-shadow:0px 0px 10px #f00, inset 0px 0px 30px #f00;
}

[api-component="CourseContent"] [api-attribute="Breadcrums"] [api-attr-array-index]{
    padding:15px 8px;
    background:#c8102e;
    color:white;
    display:inline-block;
    font-size:13px;
    cursor:pointer;
    font-family:"Trajan Pro";
    box-shadow:inset 0px 0px 0px #c8102e;
    border-right:1px solid #ddd;
    border-left:1px solid #ddd;
    transition:all 500ms;
}
[api-component="CourseContent"] [api-attribute="Breadcrums"] [api-attr-array-index]:nth-child(n+1){
    border-left:hidden;
}

.icon-for-item{
    float:left;
    height:20px;
    padding-right:20px;
}
.icon-for-item img{
    height:35px;
}

[api-component="CourseContent"] [type='week'] .icon-for-item,
[api-component="CourseContent"] [children_type='week'] .icon-for-item{
    background-image: url("/img/week.png"); /* lets say 20x20 */
}
[api-component="CourseContent"] [type='course'] .icon-for-item,
[api-component="CourseContent"] [children_type='course'] .icon-for-item{
    background-image: url("/img/course.png"); /* lets say 20x20 */
}

[api-component="CourseContent"] [type='section'] .icon-for-item,
[api-component="CourseContent"] [children_type='section'] .icon-for-item{
    background-image: url("/img/section.png"); /* lets say 20x20 */
}

[api-component="CourseContent"] [type='file'] .icon-for-item,
[api-component="CourseContent"] [children_type='file'] .icon-for-item{
    background-image: url("/img/file.png"); /* lets say 20x20 */
}

[api-component="CourseContent"] [type='assessment'] .icon-for-item,
[api-component="CourseContent"] [children_type='assessment'] .icon-for-item{
    background-image: url("/img/assessment.png"); /* lets say 20x20 */
}

[api-component="CourseContent"] [type='folder'] .icon-for-item,
[api-component="CourseContent"] [children_type='folder'] .icon-for-item{
    background-image: url("/img/folder.png"); /* lets say 20x20 */
}
[api-component="CourseContent"] [type='topic'] .icon-for-item,
[api-component="CourseContent"]  [children_type='topic'] .icon-for-item{
    background-image: url("/img/topic.png"); /* lets say 20x20 */
}
[api-component="CourseContent"] [type='magic-points'] .icon-for-item,
[api-component="CourseContent"]  [children_type='magic-points'] .icon-for-item{
    background-image: url("/img/magic-points.png"); /* lets say 20x20 */
}

[api-component="CourseContent"] [type] .icon-for-item,
[api-component="CourseContent"] [children_type] .icon-for-item{
    width:30px;
    height:30px;
    margin-right:10px;
    display:inline-block;
    background-size:contain;
    background-position:50% 50%;
    background-repeat:no-repeat;

}
