    box-shadow:inset 0px 0px 30px #333;
}

[action-component="CourseContent"] [action-attribute="Breadcrums"] [action-attr-array-index]:first-child{
    border-top-left-radius:5px;
    border-bottom-left-radius:5px;
}
[action-component="CourseContent"] [action-attribute="Breadcrums"] [action-attr-array-index]:last-child{
    border-top-right-radius:5px;
    border-bottom-right-radius:5px;
    border:none;
}

[action-component="CourseContent"] [action-attribute="Breadcrums"] [action-attr-array-index]{
    padding:15px 8px;
    background:#337ab7;

    color:white;
    display:inline-block;
    font-size:10px;
    cursor:pointer;
    font-family:"Trajan Pro";
    box-shadow:inset 0px 0px 0px #3498db;
