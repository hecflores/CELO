[action-component="CourseContent"] [action-attribute="Breadcrums"]{
    padding:0px 0px;
    display:inline-block;
    box-shadow:0px 1px 1px  rgba(0,0,0,.7),inset 0px 1px rgba(255,255,255,.7);
    border-radius:5px;
    background:#337ab7;
    color:white;
}
