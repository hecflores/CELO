.delete-btn{
    display:inline-block;
    width:20px;
    height:20px;
    background:red;
    border-radius:20px;
    line-height:20px;
    font-size:15px;
    text-align:center;
    color:white;
    font-weight:bold;
    box-shadow:0px 1px 1px rgba(0,0,0,.6),inset 0px 1px 0px rgba(255,255,255,0);
    transform:translateX(40px);
    display:none;
    transition:all 500ms;
}
.delete-btn:hover{
    box-shadow:0px 1px 1px rgba(0,0,0,.6),inset 0px 1px 20px #ff5956;
}
.delete-btn:before{
    content:"X";
}
